{
    "tags": {
        "allowUnknownTags": true
    },
    "opts": {
        "destination": "./docs/"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "path": "./node_modules/ink-docstrap/template",
        "theme": "paper",
        "navType": "vertical",
        "linenums": true,
        "dateFormat": "MMMM Do YYYY, h:mm:ss a",
        "systemName": "async-http.js",
        "copyright": "Simplify async http request using only dom attributes"
    }
}